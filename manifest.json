{

  "manifest_version": 2,
  "name": "SonicWall Login",
  "description": "Login to SonicWall the easy way.",
  "version": "1.0",
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "icons": { "16": "logo16.png",
           "48": "logo48.png",
          "128": "logo128.png" 
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "cookies",
   "activeTab",
   "alarms",
   "https://192.168.20.1/",
   "webRequest"
   ]
}
